Options +FollowSymlinks
#Options All -Indexes
<Files .htaccess>
	Order Allow,Deny
	Deny from all
</Files>
<Files constant.php>
	Order Allow,Deny
	Deny from all
</Files>
RewriteEngine on
RewriteRule ^involve$ index.php
RewriteRule ^res/(img|css|js)/(.*)?$ public/$1/$2
RewriteRule ^chat-room$ index.php?m=common&a=body
RewriteRule ^send-message$ index.php?m=message&a=main